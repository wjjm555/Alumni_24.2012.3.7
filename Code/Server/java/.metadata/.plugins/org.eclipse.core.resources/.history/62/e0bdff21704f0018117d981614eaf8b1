package com.gbs.app.model.skills;

import com.gbs.app.model.Base;
import com.gbs.app.model.players.Role;

public abstract class Skill extends Base {

	private final Type type;

	public Skill(Type type) {
		this.type = type;
	}

	public enum Type {
		Common(""), Master(""), Awaken(""), Lock("");
		private final String name;

		private Type(String name) {
			this.name = name;
		}
	}

	public Type getType() {
		return type;
	}

	public abstract Object execute(Role... roles);
}
